{
  "name": "cdk-power-constructs",
  "version": "0.1.17",
  "description": "Reusable AWS CDK constructs for common infrastructure patterns",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run build:handlers && jsii",
    "build:handlers": "cd custom-resource-handlers && npm install && npm run build",
    "watch": "jsii -w",
    "test": "npm run test:handlers && npm run test:constructs",
    "test:handlers": "cd custom-resource-handlers && npm test",
    "test:constructs": "jest",
    "test:coverage": "npm run test:coverage:handlers && npm run test:coverage:constructs && npm run coverage:merge",
    "test:coverage:handlers": "cd custom-resource-handlers && npm run test:coverage",
    "test:coverage:constructs": "jest --coverage",
    "coverage:merge": "mkdir -p .nyc_output && cp custom-resource-handlers/coverage/coverage-final.json .nyc_output/handlers.json && cp coverage/coverage-final.json .nyc_output/constructs.json && nyc report --reporter=html --reporter=text --reporter=lcov --report-dir=coverage",
    "package": "jsii-pacmak",
    "docs": "npm run docs:api && npm run docs:build",
    "docs:api": "typedoc --out website/static/api/typescript",
    "docs:build": "cd website && npm run build",
    "docs:dev": "cd website && npm start",
    "lint": "eslint lib/**/*.ts custom-resource-handlers/lib/**/*.ts test/**/*.ts",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "author": {
    "name": "Jérôme GUYON",
    "email": "jerome.guyon@guyon-it-consulting.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/guyon-it-consulting/cdk-power-constructs.git"
  },
  "keywords": [
    "awscdk",
    "aws-cdk",
    "cdk",
    "constructs"
  ],
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "cdk-power-constructs",
        "module": "cdk_power_constructs"
      },
      "java": {
        "package": "fr.guyonitconsulting.cdkpowerconstructs",
        "maven": {
          "groupId": "fr.guyon-it-consulting",
          "artifactId": "cdk-power-constructs"
        }
      },
      "dotnet": {
        "namespace": "GuyonItConsulting.CdkPowerConstructs",
        "packageId": "GuyonItConsulting.CdkPowerConstructs"
      },
      "go": {
        "moduleName": "github.com/guyon-it-consulting/cdk-power-constructs-go"
      }
    },
    "tsc": {
      "outDir": "lib",
      "rootDir": "lib"
    },
    "excludeTypescript": [
      "test",
      "custom-resource-handlers"
    ]
  },
  "exports": {
    ".": "./lib/index.js",
    "./glue": "./lib/glue/index.js"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.24",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "aws-cdk-lib": "2.238.0",
    "constructs": "^10.0.0",
    "eslint": "^9.20.0",
    "jest": "^29.7.0",
    "jsii": "^5.7.10",
    "jsii-pacmak": "^1.108.0",
    "nyc": "^17.1.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typedoc": "^0.28.17",
    "typedoc-theme-hierarchy": "^6.0.0",
    "typescript": "~5.7.2"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.238.0",
    "constructs": "^10.0.0"
  },
  "overrides": {
    "@aws-cdk/cloud-assembly-schema": "^48.20.0"
  },
  "stability": "experimental",
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.d.ts.map",
    "lib/**/*.js.map",
    "dist",
    ".jsii",
    "custom-resource-handlers/dist"
  ]
}
